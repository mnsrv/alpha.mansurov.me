{% import "../mixins/flash-messages.twig" as FlashMessages %}
{% spaceless %}
<!doctype html>
<html>
	<head>
		<meta charset="utf-8">
		<title>{{ title|default("Mansurov.me Beta") }}</title>
		{% block css %}{% endblock %}
		{% block head %}{% endblock %}
	</head>
	<body>
    <div id="body">
			{% block intro %}{% endblock %}
			{{ FlashMessages.renderMessages(messages) }}
			{% block content %}{% endblock %}
		</div>
		{% block js %}{% endblock %}
	</body>
</html>
{% endspaceless %}
